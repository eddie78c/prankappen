{
  "expo": {
    "name": "PrankBank",
    "slug": "prankbank",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "prankbank",
    "userInterfaceStyle": "automatic",
    "description": "A hilarious banking app with built-in prank features that combines financial management with sound-based pranks.",
    "primaryColor": "#007AFF",
    "backgroundColor": "#ffffff",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nideart.pranko"
    },
    "android": {
      "package": "com.nideart.pranko"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-image-picker",
      "expo-av",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b4744d5b-1812-456b-8bfe-c442ad4ad3a2"
      }
    }
  }
}
