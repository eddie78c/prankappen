{
  "expo": {
    "name": "PrankBank",
    "slug": "prankbank",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "prankbank",
    "userInterfaceStyle": "automatic",
    "description": "A hilarious banking app with built-in prank features that combines financial management with sound-based pranks.",
    "primaryColor": "#007AFF",
    "backgroundColor": "#ffffff",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nideart.pranko"
    },
    "android": {
      "package": "com.nideart.pranko",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-image-picker",
      "expo-av",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3ad71390-d2c7-4518-9d81-ed9682bbc6aa"
      },
      "router": {}
    },
    "owner": "nide.art",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3ad71390-d2c7-4518-9d81-ed9682bbc6aa"
    }
  }
}
